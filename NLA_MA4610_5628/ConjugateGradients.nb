   3.879590497074368*^9, 3.879590533294592*^9}, {3.8795915529884467`*^9, 
   3.8795916298435755`*^9}},ExpressionUUID->"950e8397-fee1-4755-ac38-\
e05a91b1d224"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"m", "=", "7"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "m"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"P", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "m"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"P", "\[LeftDoubleBracket]", 
    RowBox[{"All", ",", "1"}], "\[RightDoubleBracket]"}], "=", 
   RowBox[{"Normalize", "[", 
    RowBox[{"RandomReal", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "m"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"y", "=", 
     RowBox[{"A", ".", 
      RowBox[{"P", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", "i"}], "\[RightDoubleBracket]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"z", "=", 
     RowBox[{"y", "-", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"y", ".", 
          RowBox[{"P", "\[LeftDoubleBracket]", 
           RowBox[{"All", ",", "j"}], "\[RightDoubleBracket]"}]}], " ", 
         RowBox[{"P", "\[LeftDoubleBracket]", 
          RowBox[{"All", ",", "j"}], "\[RightDoubleBracket]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "i"}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"P", "\[LeftDoubleBracket]", 
      RowBox[{"All", ",", 
       RowBox[{"i", "+", "1"}]}], "\[RightDoubleBracket]"}], "=", 
     RowBox[{"Normalize", "[", "z", "]"}]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"i", ",", "1", ",", 
     RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"MatrixPlot", "[", 
  RowBox[{
   RowBox[{"Chop", "[", 
    RowBox[{"P", ".", 
     RowBox[{"P", "\[Transpose]"}]}], "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotLabel", "->", 
    RowBox[{"Norm", "[", 
     RowBox[{
      RowBox[{"P", ".", 
       RowBox[{"P", "\[Transpose]"}]}], "-", 
      RowBox[{"IdentityMatrix", "[", "m", "]"}]}], "]"}]}]}], "]"}]}], "Input",
