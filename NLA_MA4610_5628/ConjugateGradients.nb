Cell[CellGroupData[{

Cell["Bi-Conjugate Directions: What are they good for?", "Section",
 CellChangeTimes->{{3.8795213014688363`*^9, 3.8795213095143857`*^9}, {
  3.8795215010444903`*^9, 3.879521504364625*^9}, {3.8795869944401712`*^9, 
  3.8795870274486046`*^9}, {3.8795879270844955`*^9, 
  3.87958792783439*^9}},ExpressionUUID->"cdada7eb-71a0-4c02-9899-\
303466d58f8b"],

Cell[TextData[{
 "If two sets of ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3e6e936d-c0a4-460f-acff-08532f6e8fb5"],
 " LI vectors ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["p", 
    RowBox[{"1", ":", "m"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f87e31b0-db30-4d98-a430-a551ddaecaf5"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["p", "^"], 
    RowBox[{"1", ":", "m"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "663d1b5e-a307-4d6e-ba4d-07e36e7ce937"],
 " in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "m"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "381d0e91-7550-4dc9-a037-05201e57cdb9"],
 " are bi-conjugate then solution ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c051efeb-563e-4017-9b2b-dea1267a8f92"],
 " of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", ".", "x"}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "07f4e525-3d39-4c95-9e58-54731d7e82a4"],
 " can be written \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "m"], 
     RowBox[{
      SubscriptBox["\[Alpha]", "j"], 
      SubscriptBox["p", "j"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f1dbe3a8-685b-46ca-bda7-b2eb10fbe418"],
 ".\nMultiply by ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4b1662db-bf2b-4861-8262-81abed5e6b6e"],
 " and plug into ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", ".", "x"}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c4e086a2-f664-4ef4-8e13-f8d3ff7b43e7"],
 " to get\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "m"], 
     RowBox[{
      SubscriptBox["\[Alpha]", "j"], 
      RowBox[{"A", ".", 
       SubscriptBox["p", "j"]}]}]}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e0f60c16-ca98-4051-a28e-4777930d6f97"],
 "\nand the take the inner product with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["p", "^"], "i"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "048b9509-25a6-42bf-a8ef-66bde49cfa7f"],
 " to get\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "m"], 
     RowBox[{
      SubscriptBox["\[Alpha]", "j"], 
      RowBox[{
       SubscriptBox[
        OverscriptBox["p", "^"], "i"], ".", "A", ".", 
       SubscriptBox["p", "j"]}]}]}], "=", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["p", "^"], "i"], ".", "b"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "77097328-29d5-4ab9-b92b-7c94d6b716de"],
 ".\nThe bi-conjugate direction condition ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["p", "^"], "i"], ".", "A", ".", 
     SubscriptBox["p", "j"]}], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "47b3a39f-3950-4b76-a05e-998f2667d8d0"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "\[NotEqual]", "j"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9acdd70a-3d30-402d-b279-5e3753469ffe"],
 " eliminates the sum giving\n\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "m"], 
     RowBox[{
      SubscriptBox["\[Alpha]", "j"], 
      RowBox[{
       SubscriptBox[
        OverscriptBox["p", "^"], "i"], ".", "A", ".", 
       SubscriptBox["p", "j"]}]}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Alpha]", "i"], 
      RowBox[{
       SubscriptBox[
        OverscriptBox["p", "^"], "i"], ".", "A", ".", 
       SubscriptBox["p", "i"]}]}], "=", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["p", "^"], "i"], ".", "b"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "77331878-3707-4c99-a303-a3d3933b5b12"],
 "\nand finally the simple recipe for the coefficients\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "i"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox[
       OverscriptBox["p", "^"], "i"], ".", "b"}], 
     RowBox[{
      SubscriptBox[
       OverscriptBox["p", "^"], "i"], ".", "A", ".", 
      SubscriptBox["p", "i"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3cfea271-2f43-4d59-9b2c-74a536ebf8c7"],
 "."
}], "Text",
 CellChangeTimes->{{3.879521319864833*^9, 3.879521395579519*^9}, {
  3.8795214433783693`*^9, 3.8795217335967474`*^9}, {3.879587034109315*^9, 
  3.879587706494594*^9}, {3.8795879376586514`*^9, 
  3.8795881106761475`*^9}},ExpressionUUID->"47e93b57-fcd4-4b26-951d-\
1866f4186824"]
