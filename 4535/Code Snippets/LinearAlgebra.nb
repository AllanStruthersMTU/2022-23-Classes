Cell["The first question is why are these all the same", "Text",
 CellChangeTimes->{{3.8714630308986845`*^9, 
  3.8714630549071193`*^9}},ExpressionUUID->"68d1d7f7-5fbc-4041-831a-\
fff545865325"],

Cell[TextData[{
 "Equivalence of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"A", " ", "t"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "df58eba8-cede-4e0b-b94d-627b7739a89b"],
 "Definitions (Informal)\n\n1) Differentiate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox["A", "n"], 
      FractionBox[
       SuperscriptBox["t", "n"], 
       RowBox[{"n", "!"}]]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "14669053-1ea8-410a-b10f-286af977f42f"],
 "  to get\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", "dt"], "[", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"n", "=", "0"}], "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["A", "n"], 
       FractionBox[
        SuperscriptBox["t", "n"], 
        RowBox[{"n", "!"}]]}]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["d", "dt"], "[", 
      RowBox[{
       SuperscriptBox["A", "0"], "+", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"n", "=", "1"}], "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["A", "n"], 
         FractionBox[
          SuperscriptBox["t", "n"], 
          RowBox[{"n", "!"}]]}]}]}], "]"}], "=", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"n", "=", "1"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox["A", "n"], 
        FractionBox[
         RowBox[{"n", " ", 
          SuperscriptBox["t", 
           RowBox[{"n", "-", "1"}]]}], 
         RowBox[{"n", "!"}]]}]}], "=", 
      RowBox[{
       RowBox[{"A", ".", 
        RowBox[{
         SubsuperscriptBox["\[Sum]", 
          RowBox[{"n", "=", "1"}], "\[Infinity]"], 
         RowBox[{
          SuperscriptBox["A", 
           RowBox[{"n", "-", "1"}]], 
          FractionBox[
           RowBox[{" ", 
            SuperscriptBox["t", 
             RowBox[{"n", "-", "1"}]]}], 
           RowBox[{"n", "-", 
