Cell[TextData[{
 "For a scalar ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[Alpha]", " ", "t"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2de11906-251a-4df1-b8ad-f7992052164e"],
 " can be defined by either the IVP \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"\[Alpha]", " ", "y"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f1a2e8ed-190d-46da-8acf-6a6f42efe8d1"],
 "  and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "0", ")"}], "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b2c6d38e-bc68-439f-aece-b9e674c39af0"],
 "\nor the power series\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[Alpha]", " ", "t"}]], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "\[Infinity]"], 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Alpha]", " ", "t"}], ")"}], "n"], 
      RowBox[{"n", "!"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6fac405e-f615-41ad-997f-dd4d31079eea"],
 ". \t"
}], "Text",
 CellChangeTimes->{{3.871462369383052*^9, 
  3.8714625298794127`*^9}},ExpressionUUID->"8536ff8b-f4e8-40fe-b15d-\
b76b21ebfb40"],

Cell[TextData[{
 "For a square matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "95a456bc-11db-4fbb-9e82-65987fcfa22e"],
 " the matrix exponential ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"A", " ", "t"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b2d43de1-9c66-46a9-b15c-1af1c9705542"],
 " is defined as the solution of the matrix IVP\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Y", "'"}], "=", 
    RowBox[{"A", ".", "Y"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d556455d-87d5-4e6f-af9a-9420c6d5aba8"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Y", "(", "0", ")"}], "=", 
    SubscriptBox["I", "m"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "65689347-6bba-4ec8-a60b-1179ab3c8259"],
 "\nor the matrix power series \n\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"A", " ", "t"}]], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"A", " ", "t"}], ")"}], "n"], 
       RowBox[{"n", "!"}]]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["A", "n"], 
       FractionBox[
        SuperscriptBox["t", "n"], 
        RowBox[{"n", "!"}]]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0eee569d-ed0d-4816-b482-19aadbd53a72"],
 "."
}], "Text",
