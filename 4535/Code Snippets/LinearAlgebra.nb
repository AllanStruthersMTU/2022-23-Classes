    RowBox[{"\[Lambda]s", ",", "V"}], "}"}], "=", 
   RowBox[{"Eigensystem", "[", "A", "]"}]}], ";", 
  RowBox[{"V", "=", 
   RowBox[{"V", "\[Transpose]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Norm", "[", 
  RowBox[{"A", "-", 
   RowBox[{"V", ".", 
    RowBox[{"DiagonalMatrix", "[", "\[Lambda]s", "]"}], ".", 
    RowBox[{"Inverse", "[", "V", "]"}]}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.8714605197328043`*^9, 3.871460601624696*^9}},
