  FormBox[
   RowBox[{
    SubscriptBox["F", "i"], "=", 
    RowBox[{"C", " ", 
     SubscriptBox["m", "i"], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "n"], 
      RowBox[{
       SubscriptBox["m", "j"], 
       FractionBox[
        RowBox[{
         SubscriptBox["p", "i"], "-", 
         SubscriptBox["p", "j"]}], 
        RowBox[{"||", 
         RowBox[{
          SubscriptBox["p", "i"], "-", 
          SubscriptBox["p", "j"]}], 
         SuperscriptBox["||", "3"]}]]}]}]}]}], TraditionalForm]],
