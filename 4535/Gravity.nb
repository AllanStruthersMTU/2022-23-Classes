Cell[TextData[{
 "Newton\[CloseCurlyQuote]s Laws\n\nFor each body ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "i"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["m", "i"], " ", 
      SubscriptBox["a", "i"]}], "=", 
     RowBox[{
      SubscriptBox["m", "i"], 
      SubsuperscriptBox["p", "i", "''"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "553c52d3-dec4-4706-bec7-a7bbed2ad3e3"],
