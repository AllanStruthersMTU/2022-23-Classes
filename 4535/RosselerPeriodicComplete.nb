Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "[", 
   RowBox[{"{", 
    RowBox[{"y1_", ",", "y3_"}], "}"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"yP", ",", "JP", ",", 
      RowBox[{"w", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"yP", ",", "JP"}], "}"}], "=", 
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"Reap", "[", 
         RowBox[{"NDSolveValue", "[", 
          RowBox[{
           RowBox[{"{", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"y", "'"}], "[", "t", "]"}], "==", 
              RowBox[{
               RowBox[{"f", "[", "StdParams", "]"}], "[", 
               RowBox[{"y", "[", "t", "]"}], "]"}]}], ",", " ", 
             RowBox[{
              RowBox[{"y", "[", "0", "]"}], "==", 
              RowBox[{"{", 
               RowBox[{"y1", ",", "0", ",", "y3"}], "}"}]}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"J", "'"}], "[", "t", "]"}], "==", 
