           RowBox[{"{", "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", "0", ",", " ", "TMax"}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Method", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{"\"\<EventLocator\>\"", ",", " ", 
