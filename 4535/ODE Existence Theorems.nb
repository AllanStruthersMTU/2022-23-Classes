Cell["\<\
Your standard ODE text states (but does not prove) a theorem that says \
\>", "Text",
 CellChangeTimes->{{3.870946300155486*^9, 
  3.8709463382969055`*^9}},ExpressionUUID->"ea0fd11a-ce54-4d46-ac18-\
0917dc4bb1e0"],

Cell[TextData[{
 "Standard Theorem (Picard)\nI",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", ":", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], "\[Rule]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"1ec197a3-0f48-409d-9878-09cdba8dd620"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Df", ":", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], "\[Rule]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", 
     RowBox[{"n", "\[Times]", "n"}]]}], TraditionalForm]],ExpressionUUID->
  "e9212d29-9e26-4ae7-885f-e4b150b0f012"],
 " are continuous in some neighbourhood of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "0"], "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e50f5852-ce3e-45a1-bfc5-b0cab8a8a435"],
 "then the IVP ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"f", "(", "y", ")"}]}], TraditionalForm]],ExpressionUUID->
  "26b63538-0822-4453-8852-87dbcdd015bd"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], 
     RowBox[{"(", 
      SubscriptBox["t", "0"], ")"}]}], "=", 
    SubscriptBox["y", "0"]}], TraditionalForm]],ExpressionUUID->
  "feac6ea4-2039-4c0b-9c97-7d18951ca6c6"],
 " has a unique solution on some interval around ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "0"], TraditionalForm]],ExpressionUUID->
  "042eeb8b-4851-4913-b3f2-b6d91468e801"],
 "."
}], "Abstract",
 CellChangeTimes->{{3.8708770816673837`*^9, 3.8708772057760105`*^9}, {
  3.8708787684447575`*^9, 3.8708787693346977`*^9}, {3.8709463522006807`*^9, 
  3.8709465320781083`*^9}, {3.870946630640123*^9, 
  3.8709466380941896`*^9}},ExpressionUUID->"a9c79119-11cb-4ba3-a723-\
f0c10548ecf9"],

Cell["\<\
Some books might state (but again not prove) a slightly different theorem\
\>", "Text",
 CellChangeTimes->{{3.8709465679920464`*^9, 
  3.8709466191712046`*^9}},ExpressionUUID->"4ba976a2-6af7-4975-873f-\
42860a35b4b8"],

Cell[TextData[{
 "Theorem (Peano)\nI",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", ":", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], "\[Rule]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"4cfa270d-a8fa-4389-8900-3f2459e40ed3"],
 " is continuous in some neighbourhood of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "0"], "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d54b3e21-0062-461e-8921-53ba8553a1c9"],
 "then the IVP ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"f", "(", "y", ")"}]}], TraditionalForm]],ExpressionUUID->
  "15afcb20-6d33-479a-9451-aa1e5c405a72"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], 
     RowBox[{"(", 
      SubscriptBox["t", "0"], ")"}]}], "=", 
    SubscriptBox["y", "0"]}], TraditionalForm]],ExpressionUUID->
  "ca0360f7-b99d-4e33-841d-eda8e835e0e6"],
 " has a solution on some interval around ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "0"], TraditionalForm]],ExpressionUUID->
  "3b5b7d3d-709c-4228-8954-811ddb9a9aa3"],
 "."
}], "Abstract",
 CellChangeTimes->{{3.8708770816673837`*^9, 3.8708772057760105`*^9}, {
  3.8708787684447575`*^9, 3.8708787693346977`*^9}, {3.8709463522006807`*^9, 
  3.8709465320781083`*^9}, {3.870946675230299*^9, 
  3.870946712569327*^9}},ExpressionUUID->"87cc2c5d-c106-4028-b372-\
5a9eac47e8a5"],

Cell["\<\
We are going to build up (and demonstrate why they work) numerous ODE \
theorems. \
\>", "Text",
 CellChangeTimes->{{3.8709467620210505`*^9, 
  3.870946818333429*^9}},ExpressionUUID->"36d25fc5-26df-4a9b-9557-\
ae469c29f1c5"],

Cell[CellGroupData[{

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"f", "(", "y", ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "59b442a2-3e22-43f2-9da4-398a1f2ed9d9"],
 "with smooth f"
}], "Subsection",
 CellChangeTimes->{{3.870878597118907*^9, 3.870878623773615*^9}, {
  3.870879904746928*^9, 3.870879918108637*^9}, {3.870880256796488*^9, 
  3.870880270833767*^9}, {3.87088052592938*^9, 3.8708805407864923`*^9}, {
  3.8709468232213345`*^9, 
  3.8709468520465317`*^9}},ExpressionUUID->"d6b13fa9-3136-4179-abff-\
7a4b756970b6"],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"f", "(", "y", ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "12fd5cc0-59ae-4e08-bdf9-2eb1ad98e1bd"],
 " and f is smooth then differentiating with respect to time gives \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "''"}], "=", 
    RowBox[{
     RowBox[{"Df", "(", "y", ")"}], ".", 
     RowBox[{"y", "'"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1b4c683b-e134-4042-97ee-57d0ca47377c"],
 ".\nDifferentiating again gives \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'''"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"Df", "(", "y", ")"}], ".", 
      RowBox[{"y", "''"}]}], "+", 
     RowBox[{
      SuperscriptBox["D", "2"], 
      RowBox[{
       RowBox[{"f", "(", "y", ")"}], ".", 
       RowBox[{"y", "'"}], ".", 
       RowBox[{"y", "'"}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c2474248-93cb-48d3-8835-90e9f8f13f2e"],
 "\nRepeating this for an infinitely smooth function ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "833d8353-db08-49e7-bdb9-105b9563c9f0"],
 " and evaluating at ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", 
     SubscriptBox["t", "0"], ")"}], "=", 
    SubscriptBox["y", "0"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c5aeab88-9d4b-4310-bfcd-99f0609fdc75"],
 " gives a sequence of derivative values\n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      RowBox[{"y", "(", 
       SubscriptBox["t", "0"], ")"}], "=", 
      SubscriptBox["y", "0"], " ", " "},
     {
      RowBox[{
       RowBox[{"y", "'"}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "=", 
      RowBox[{"f", "(", 
       SubscriptBox["y", "0"], ")"}], " ", " "},
     {
      RowBox[{
       RowBox[{"y", "''"}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Df", "(", 
         SubscriptBox["y", "0"], ")"}], ".", 
        RowBox[{"y", "'"}]}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], " ", " "},
     {
      RowBox[{
       RowBox[{"y", "'''"}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Df", "(", 
         SubscriptBox["y", "0"], ")"}], ".", 
        RowBox[{"y", "''"}]}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "+", 
      RowBox[{
       SuperscriptBox["D", "2"], 
       RowBox[{
        RowBox[{"f", "(", 
         SubscriptBox["y", "0"], ")"}], ".", 
        RowBox[{"y", "'"}]}], 
       RowBox[{
        RowBox[{"(", 
         SubscriptBox["t", "0"], ")"}], ".", 
        RowBox[{"y", "'"}]}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}]},
     {
      RowBox[{
       RowBox[{"y", "''''"}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Df", "(", 
         SubscriptBox["y", "0"], ")"}], ".", 
        RowBox[{"y", "''"}]}], 
       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}]}], "+", "\[Ellipsis]"},
     {"\[Ellipsis]", " ", " ", " ", " "}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a956bf18-94be-42f5-ac54-99b7cdab5a16"],
 "\nFor a smooth function ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1e2185b9-e24c-4b44-ac4f-d59766e5c018"],
 " we can compute a truncated Taylor Series for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "(", "t", ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2c18c69d-5418-418c-906e-405e84d1a05b"],
 "\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{"y", "(", 
      SubscriptBox["t", "0"], ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SubscriptBox["t", "0"]}], ")"}], 
      RowBox[{"y", "'"}], 
      RowBox[{"(", 
       SubscriptBox["t", "0"], ")"}]}], "+", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"t", "-", 
          SubscriptBox["t", "0"]}], ")"}], "2"], 
       RowBox[{"2", "!"}]], 
      RowBox[{"y", "''"}], 
      RowBox[{"(", 
       SubscriptBox["t", "0"], ")"}]}], "+", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"t", "-", 
          SubscriptBox["t", "0"]}], ")"}], "3"], 
       RowBox[{"3", "!"}]], 
      RowBox[{"y", "'''"}], 
      RowBox[{"(", 
       SubscriptBox["t", "0"], ")"}]}], "+", "\[Ellipsis]"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "dc9e51bc-10cf-409a-9ea5-bd48aa11ab39"],
 "\nYou learnt a lot about the Taylor Series \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"t", "-", 
          SubscriptBox["t", "0"]}], ")"}], "n"], 
       RowBox[{"n", "!"}]], 
      RowBox[{
       SuperscriptBox["y", 
        RowBox[{"(", "n", ")"}]], "(", 
       SubscriptBox["t", "0"], ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "252fce2b-889f-4de9-a422-081ec021cc3d"],
 "\nin Calculus II! "
}], "Text",
 CellChangeTimes->{{3.870946861574284*^9, 3.8709470229463387`*^9}, {
