Cell[CellGroupData[{

Cell["2nd Order \[Rule] 1st Order", "Subsection",
 CellChangeTimes->{{3.870878597118907*^9, 3.870878623773615*^9}, {
  3.870879904746928*^9, 3.870879918108637*^9}, {3.870880256796488*^9, 
  3.870880270833767*^9}, {3.87088052592938*^9, 3.8708805407864923`*^9}, {
  3.8708808683895636`*^9, 3.870880873257391*^9}, {3.8708812909938917`*^9, 
  3.8708813076974487`*^9}, {3.870881339445551*^9, 3.8708813490313816`*^9}, {
  3.8708814090457497`*^9, 3.870881413519906*^9}, {3.8709461752964215`*^9, 
  3.870946194909601*^9}},ExpressionUUID->"7d63053b-dfe4-4265-a006-\
c761f4f5977d"],

Cell[TextData[{
 "A general second order ODE is \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "''"}], "=", 
    RowBox[{"g", "(", 
     RowBox[{"t", ",", "y", ",", 
      RowBox[{"y", "'"}]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a737a666-2a66-47f5-9cd7-d9677b38882d"],
 "\nPutting ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{"y", "'"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "de6c6ef5-20ad-4bb5-9da3-958dceb6e49d"],
 " gives \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "'"}], "=", 
    RowBox[{"g", "(", 
     RowBox[{"t", ",", "y", ",", "v"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3b388a0d-bed1-4f75-96bc-14449783cfbc"],
 "\nwhich combines with the definition of the velocities to give the double \
sized system\n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      RowBox[{"y", "'"}], "=", "v"},
     {
      RowBox[{"v", "'"}], "=", 
      RowBox[{"g", "(", 
       RowBox[{"t", ",", "y", ",", "v"}], ")"}]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "df5deef7-d404-42e9-a0b6-b5540ef5bf5e"],
 "\nWriting ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["y", "^"], "=", 
    RowBox[{"[", 
     RowBox[{"y", ";", "v"}], "]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d2c263ae-a7fc-48e7-853b-635ba30e6864"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["f", "^"], "(", 
     RowBox[{"t", ",", 
      OverscriptBox["y", "^"]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      OverscriptBox["f", "^"], "(", 
      RowBox[{"t", ",", 
       RowBox[{"[", 
        RowBox[{"y", ";", "v"}], "]"}]}], ")"}], ":=", 
     RowBox[{"[", 
      RowBox[{"v", ";", 
       RowBox[{"g", "(", 
        RowBox[{"t", ",", "y", ",", "v"}], ")"}]}], "]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b946d9b0-c18b-4ae4-8492-b868fa408aa5"],
 " gives our standard form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["y", "^"], "'"}], "=", 
    RowBox[{
     OverscriptBox["f", "^"], "(", 
     RowBox[{"t", ",", 
      OverscriptBox["y", "^"]}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "49927843-d5f0-43ad-b024-a3ca83230e70"],
 "."
}], "Text",
 CellChangeTimes->{{3.8708770816673837`*^9, 3.8708772057760105`*^9}, {
  3.8708786415817585`*^9, 3.8708787007122946`*^9}, {3.8708800469073315`*^9, 
  3.8708801402852664`*^9}, {3.870880274988904*^9, 3.870880394458332*^9}, {
  3.870880428937771*^9, 3.870880436333378*^9}, {3.870880551153406*^9, 
  3.8708807193831935`*^9}, {3.870880879448161*^9, 3.8708811679067945`*^9}, {
  3.87088122805138*^9, 3.8708812625150547`*^9}, {3.8708813201142445`*^9, 
  3.8708814020700645`*^9}, {3.870881600848752*^9, 3.8708816559123545`*^9}, {
  3.87088168804885*^9, 
  3.8708820107605133`*^9}},ExpressionUUID->"fe18e4ad-b429-4644-bfbc-\
3e88d7a3e578"],

Cell["The same trick works for higher order ODEs.", "Item",
