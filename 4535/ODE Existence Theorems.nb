Cell[CellGroupData[{

Cell["Autonomous Form", "Subsection",
 CellChangeTimes->{{3.870878597118907*^9, 3.870878623773615*^9}, {
  3.870879904746928*^9, 3.870879918108637*^9}, {3.870880256796488*^9, 
  3.870880270833767*^9}, {3.87088052592938*^9, 3.8708805407864923`*^9}, {
  3.8708808683895636`*^9, 
  3.870880873257391*^9}},ExpressionUUID->"342e4c13-9354-4398-a10c-\
d6e424540137"],

Cell[TextData[{
 "A nonautonomous IVP \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{"f", "(", 
     RowBox[{"t", ",", "y"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "5f44cf6f-edb9-4dc9-a3a9-c011771b1813"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", 
     SubscriptBox["t", "0"], ")"}], "=", 
    RowBox[{
     SubscriptBox["y", "0"], "\[Element]", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "31302874-41a2-4b82-a5b5-e44f411f8e8c"],
 "\ncan be rewritten as the equivalent autonomous IVP  \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["y", "^"], "'"}], "=", 
    RowBox[{
     OverscriptBox["f", "^"], "(", 
     OverscriptBox["y", "^"], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cd0431d0-90c1-42ce-b3aa-32c0901f4832"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["y", "^"], "(", 
     SubscriptBox["t", "0"], ")"}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       SubscriptBox["t", "0"], ";", 
       SubscriptBox["y", "0"]}], "]"}], "\[Element]", 
     SuperscriptBox["\[DoubleStruckCapitalR]", 
      RowBox[{"n", "+", "1"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a31887bf-48de-4235-b30a-5d639af0a0ef"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["y", "^"], "(", "t", ")"}], "=", 
    RowBox[{"[", 
     RowBox[{"t", ";", 
      RowBox[{"y", "(", "t", ")"}]}], "]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "50dd93c8-6e2c-4907-8356-e2ce9c077d5d"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"t", ",", "y"}], ")"}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"1", ";", "y"}], "]"}], ":=", 
     RowBox[{
      OverscriptBox["f", "^"], "(", 
      OverscriptBox["y", "^"], ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9b88c946-6108-41bd-b0d7-201c160ba426"],
 ". "
}], "Text",
 CellChangeTimes->{{3.8708770816673837`*^9, 3.8708772057760105`*^9}, {
  3.8708786415817585`*^9, 3.8708787007122946`*^9}, {3.8708800469073315`*^9, 
