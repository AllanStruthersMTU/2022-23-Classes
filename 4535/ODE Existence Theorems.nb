Cell[TextData[{
 "Informal Theorem\nAn ODE ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", 
    RowBox[{"f", "(", 
     RowBox[{"t", ",", 
      RowBox[{"y", "(", "t", ")"}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"813d04f5-0318-40f6-8a37-6be328a0ad22"],
