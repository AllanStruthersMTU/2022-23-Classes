      SubscriptBox["t", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["t", "0"], "t"], " ", 
      RowBox[{
       RowBox[{"y", "'"}], 
       RowBox[{"(", "\[Tau]", ")"}], "d\[Tau]"}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["t", "0"], "t"], " ", 
      RowBox[{
       RowBox[{"f", "(", 
        RowBox[{"\[Tau]", ",", 
         RowBox[{"y", "(", "\[Tau]", ")"}]}], ")"}], "d\[Tau]"}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "271a1eca-0fd8-4cc5-a5fc-18b90628cca8"],
 "\nwhich is usually rewritten \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{"y", "(", 
      SubscriptBox["t", "0"], ")"}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["t", "0"], "t"], " ", 
      RowBox[{
       RowBox[{"f", "(", 
        RowBox[{"\[Tau]", ",", 
         RowBox[{"y", "(", "\[Tau]", ")"}]}], ")"}], "d\[Tau]"}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "870147d8-312f-48a1-8735-12a342dea756"],
 "."
}], "Text",
 CellChangeTimes->{{3.8708770816673837`*^9, 3.8708772057760105`*^9}, {
