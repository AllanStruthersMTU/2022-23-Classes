         RowBox[{"2", "/", "3"}], "0"}
       }], ")"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"1", "/", "4"}], ",", 
       RowBox[{"3", "/", "4"}]}], "}"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "y_", "]"}], ":=", " ", "y"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"t", ",", "y"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"0.0", ",", "1.0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"h", "=", "0.1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RalstonStep", "[", "f", "]"}], "[", 
  RowBox[{"{", 
   RowBox[{"h", ",", "t", ",", "y"}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
