Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"RalstonStep", "[", "f_", "]"}], "[", 
   RowBox[{"{", 
    RowBox[{"h_", ",", "t_", ",", "y_"}], "}"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"K1", ",", "K2"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"K1", "=", 
      RowBox[{"f", "[", "y", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"K2", " ", "=", " ", 
      RowBox[{"f", "[", 
       RowBox[{"y", "+", 
        RowBox[{"2", 
         RowBox[{"h", "/", "3"}], " ", "K1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"h", ",", 
       RowBox[{"t", "+", "h"}], ",", 
       RowBox[{"y", "+", 
        RowBox[{"h", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"1", "/", "4"}], "*", " ", "K1"}], " ", "+", 
           RowBox[{
            RowBox[{"3", "/", "4"}], "*", " ", "K2"}]}], ")"}]}]}]}], 
      "}"}]}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RalstonTableau", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"0", "0"},
