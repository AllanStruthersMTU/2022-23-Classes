Cell[BoxData[{
 RowBox[{
  RowBox[{"BogakiShampineTableau", "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", "\[NoBreak]", GridBox[{
        {"0", "0", "0", "0"},
        {
         RowBox[{"1", "/", "2"}], "0", "0", "0"},
        {"0", 
         RowBox[{"3", "/", "4"}], "0", "0"},
        {
         RowBox[{"2", "/", "9"}], 
         RowBox[{"1", "/", "3"}], 
         RowBox[{"4", "/", "9"}], "0"}
       }], "\[NoBreak]", ")"}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "/", "9"}], ",", 
       RowBox[{"1", "/", "3"}], ",", 
       RowBox[{"4", "/", "9"}], ",", "0"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"7", "/", "24"}], ",", 
       RowBox[{"1", "/", "4"}], ",", 
       RowBox[{"1", "/", "3"}], ",", 
       RowBox[{"1", "/", "8"}]}], "}"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"RKF", "[", 
     RowBox[{"f_", ",", 
      RowBox[{"{", 
       RowBox[{"A_List", ",", "b_List", ",", "bStar_List"}], "}"}], ",", 
      "Tol_"}], "]"}], "[", 
    RowBox[{"{", 
     RowBox[{"h_", ",", "t_", ",", "y_"}], "}"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"k", "=", 
        RowBox[{"Length", "[", "A", "]"}]}], ",", "Ks"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Compute", " ", "Internal", " ", "Slopes"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"Ks", "[", "1", "]"}], "=", 
       RowBox[{"f", "[", "y", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Ks", "[", "i", "]"}], "=", 
         RowBox[{"f", "[", 
          RowBox[{"y", "+", 
           RowBox[{"h", "*", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"A", "\[LeftDoubleBracket]", 
                RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], " ", 
               RowBox[{"Ks", "[", "j", "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", 
                RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}]}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "k"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Modify", " ", "In", " ", "Class"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"h", ",", 
        RowBox[{"t", "+", "h"}], ",", 
        RowBox[{"y", "+", 
         RowBox[{"h", "*", 
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "b", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], " ", 
             RowBox[{"Ks", "[", "i", "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "k"}], "}"}]}], "]"}]}]}], ",", 
        RowBox[{"y", "+", 
         RowBox[{"h", "*", 
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "bStar", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
