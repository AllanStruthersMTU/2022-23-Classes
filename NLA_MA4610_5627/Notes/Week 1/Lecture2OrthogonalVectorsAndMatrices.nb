Cell[TextData[{
 "Suppose ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["p", "1", "2"], "+", 
     SubsuperscriptBox["p", "2", "2"]}], "=", "1"}], TraditionalForm]],
  ExpressionUUID->"3b151c28-9e55-4343-b683-4cee14fe066a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i1", "\[NotEqual]", "i2"}], TraditionalForm]],ExpressionUUID->
  "7983c227-b294-493f-bf2a-c5d8a048b9d1"],
 " then we can build an orthogonal ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "\[Times]", "m"}], TraditionalForm]],ExpressionUUID->
  "2ec2d9c1-aca9-43e2-af68-3d790d8d63f6"],
 " matrix Q as follows. "
}], "Text",
