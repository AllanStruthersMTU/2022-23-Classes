Cell[TextData[{
 "For an ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "\[Times]", "n"}], TraditionalForm]],ExpressionUUID->
  "4c00559b-bab3-4b3c-a577-22b8d5b62126"],
 " matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "82d91eba-4486-4eb0-b229-e6af942ba2f2"],
 " the adjoint ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "*"], TraditionalForm]],ExpressionUUID->
  "a3c10b88-bf1b-4e3e-8437-b9e04b437a6e"],
 " is the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Times]", "m"}], TraditionalForm]],ExpressionUUID->
  "c10c0697-590b-4ef7-ad2e-10f153fec20a"],
 " matrix obtained by interchanging rows and columns of the complex conjugate \
of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "c4d4d3d5-4f06-4d46-9865-86626d28f1d1"],
 ".  In other words\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["A", "*"], ")"}], 
     RowBox[{"i", ",", "j"}]], "=", 
    SubscriptBox[
     OverscriptBox["A", "_"], 
     RowBox[{"j", ",", "i"}]]}], TraditionalForm]],ExpressionUUID->
  "b4e3b2b3-8422-48bb-9cb4-4ffd2eb2db65"],
 "  "
}], "Text",
 CellChangeTimes->{{3.846670950587816*^9, 
