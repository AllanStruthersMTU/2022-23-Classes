568e894b50d6"],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "c8fa137c-cb73-4868-b5d6-60ed7c0eed0f"],
 " is differentiable we can write ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"J", "(", "x", ")"}], "=", 
    RowBox[{"Df", "(", "x", ")"}]}], TraditionalForm]],ExpressionUUID->
  "bdb1c0a9-1e16-40f4-837e-0699d98ecdb7"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]f", "=", 
    RowBox[{
     RowBox[{"J", "(", "x", ")"}], ".", "\[Delta]x"}]}], TraditionalForm]],
  ExpressionUUID->"86da4a43-b956-4be7-9c18-e559d706e45b"],
 " and "
}], "Text",
 CellChangeTimes->{{3.852478122345471*^9, 3.852478258708681*^9}, {
  3.8524783007648478`*^9, 
  3.8524783144923353`*^9}},ExpressionUUID->"51468994-673b-4150-a7f8-\
80efa5ffb856"],

Cell[TextData[{
 "Absolute Condition Number\n",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      OverscriptBox["\[Kappa]", "^"], "=", 
      RowBox[{
       SubscriptBox["lim", 
        RowBox[{"\[Delta]", "->", "0"}]], 
       RowBox[{
        SubscriptBox["max", 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"<", "\[Delta]"}]}]], 
        FractionBox[
         RowBox[{"||", 
          RowBox[{
           RowBox[{"f", "(", 
            RowBox[{"x", "+", "\[Delta]x"}], ")"}], "-", 
           RowBox[{"f", "(", "x", ")"}]}], "||"}], 
         RowBox[{"||", "\[Delta]x", "||"}]], 
        "                                  "}]}]},
     {" ", "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["max", "\[Delta]x"], 
        FractionBox[
         RowBox[{"||", "\[Delta]f", "||"}], 
         RowBox[{"||", "\[Delta]x", "||"}]]}], "=", 
       RowBox[{
        RowBox[{
         SubscriptBox["max", "\[Delta]x"], 
         FractionBox[
          RowBox[{"||", 
           RowBox[{
            RowBox[{"J", "(", "x", ")"}], ".", "\[Delta]x"}], "||"}], 
          RowBox[{"||", "\[Delta]x", "||"}]]}], "=", 
        RowBox[{"||", 
         RowBox[{"J", 
          RowBox[{"(", "x", ")"}]}], 
         SubscriptBox["||", "induced"]}]}]}]}
    }], TraditionalForm]],ExpressionUUID->
  "5cbc3e82-ab69-4f90-869d-445ee393cbc7"],
 "\n"
}], "Subsubsection",
 CellChangeTimes->{{3.852477408901309*^9, 3.8524774954342546`*^9}, {
  3.852477529925613*^9, 3.8524775951949673`*^9}, {3.852477718853684*^9, 
  3.8524777661466303`*^9}, {3.8524783306949787`*^9, 3.8524783455188723`*^9}, {
  3.853250790301586*^9, 3.853250790301586*^9}, {3.884355227921446*^9, 
  3.884355375472943*^9}, {3.884448684618209*^9, 3.88444869522929*^9}},
 Background->RGBColor[
  1, 0.9, 0.8],ExpressionUUID->"8d509d52-34db-49f8-acfc-47e1f5b11633"],

Cell[CellGroupData[{

Cell[TextData[{
 "Relative Condition Number\n",
 Cell[BoxData[
  FormBox[GridBox[{
     {"\[Kappa]", "=", 
      RowBox[{
       SubscriptBox["lim", 
        RowBox[{"\[Delta]", "->", "0"}]], 
       RowBox[{
        SubscriptBox["sup", 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"<", "\[Delta]"}]}]], 
        FractionBox[
         RowBox[{"||", 
          RowBox[{
           RowBox[{"f", "(", 
            RowBox[{"x", "+", "\[Delta]x"}], ")"}], "-", 
           RowBox[{"f", "(", "x", ")"}]}], "||", 
          RowBox[{"/", 
           RowBox[{"||", 
            RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"/", 
           RowBox[{"||", "x", "||"}]}]}]]}]}]},
     {" ", "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["sup", "\[Delta]x"], 
        FractionBox[
         RowBox[{"||", "\[Delta]f", "||", 
          RowBox[{"/", 
           RowBox[{"||", 
            RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"/", 
           RowBox[{"||", "x", "||"}]}]}]]}], "=", 
       FractionBox[
        RowBox[{"||", 
         RowBox[{"J", "(", "x", ")"}], 
         SubscriptBox["||", "ind"]}], 
        RowBox[{"||", 
         RowBox[{"f", "(", "x", ")"}], "||", 
         RowBox[{"/", 
          RowBox[{"||", "x", "||"}]}]}]]}]}
    }], TraditionalForm]],ExpressionUUID->
  "fdaec2e3-7872-43b9-b174-ee6137da5ba6"]
}], "Subsubsection",
 CellChangeTimes->{{3.852477408901309*^9, 3.8524774954342546`*^9}, {
  3.852477529925613*^9, 3.852477640723259*^9}, {3.852477751844701*^9, 
  3.8524777847941027`*^9}, {3.85247835734241*^9, 3.852478431065967*^9}, {
  3.8532507934031105`*^9, 3.853250796256194*^9}, {3.884355393034878*^9, 
