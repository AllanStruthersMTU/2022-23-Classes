Cell[TextData[{
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Kappa]", "b"], "(", 
     RowBox[{"A", ",", "b"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SubscriptBox["\[Sigma]", "m"]], "\[Times]", 
       FractionBox[
        RowBox[{"||", "b", "||"}], 
        RowBox[{"||", 
         RowBox[{
          SuperscriptBox["A", 
           RowBox[{"-", "1"}]], ".", "b"}], "||"}]]}], "\[LessEqual]", 
      RowBox[{
       FractionBox["1", 
        SubscriptBox["\[Sigma]", "m"]], "\[Times]", 
       SubscriptBox["max", 
        RowBox[{"||", "b", "||"}]], 
       FractionBox[
        RowBox[{"||", "b", "||"}], 
        RowBox[{"||", 
         RowBox[{
          SuperscriptBox["A", 
           RowBox[{"-", "1"}]], ".", "b"}], "||"}]]}]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SubscriptBox["\[Sigma]", "m"]], "\[Times]", 
       FractionBox["1", 
        RowBox[{"min", "[", 
         RowBox[{"1", "/", 
          SubscriptBox["\[Sigma]", 
           RowBox[{"1", ":", "m"}]]}], "]"}]]}], "=", 
      RowBox[{
       RowBox[{
        FractionBox["1", 
         SubscriptBox["\[Sigma]", "m"]], "\[Times]", 
        FractionBox["1", 
         RowBox[{"1", "/", 
          SubscriptBox["\[Sigma]", "1"]}]]}], "=", 
       FractionBox[
        SubscriptBox["\[Sigma]", "1"], 
        SubscriptBox["\[Sigma]", "m"]]}]}]}]}], TraditionalForm]],
