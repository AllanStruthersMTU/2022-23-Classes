Cell[TextData[{
 "We have formulas if ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e3c49c2f-d7b3-40bf-beb7-b8a83e7d3d4b"],
 " is smooth\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[Kappa]", "^"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["sup", "\[Delta]x"], 
      FractionBox[
       RowBox[{"||", 
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x", ")"}], " ", "\[Delta]x"}], "||"}], 
       RowBox[{"||", "\[Delta]x", "||"}]]}], "=", 
     RowBox[{"|", 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", "x", ")"}]}], "|"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "41d1aba9-6bf4-4581-bcd5-e49932875321"],
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Kappa]", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["sup", "\[Delta]x"], 
      FractionBox[
       RowBox[{"||", 
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x", ")"}], " ", "\[Delta]x"}], "||", 
        RowBox[{"/", 
         RowBox[{"||", 
          RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
       RowBox[{"||", "\[Delta]x", "||", 
        RowBox[{"/", 
         RowBox[{"||", "x", "||"}]}]}]]}], "=", 
     RowBox[{"|", 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", "x", ")"}]}], "|", 
      RowBox[{"\[Times]", 
       FractionBox[
        RowBox[{"|", "x", "|"}], 
        RowBox[{"|", 
         RowBox[{"f", "(", "x", ")"}], "|"}]]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "99a5bea4-131e-476c-978a-50f4efdf9ebe"]
