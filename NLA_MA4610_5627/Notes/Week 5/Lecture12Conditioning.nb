  3.853250790301586*^9, 3.853250790301586*^9}, {3.884355227921446*^9, 
  3.884355375472943*^9}},
 Background->RGBColor[
  1, 0.9, 0.8],ExpressionUUID->"8d509d52-34db-49f8-acfc-47e1f5b11633"],

Cell[CellGroupData[{

Cell[TextData[{
 "Relative Condition Number\n",
 Cell[BoxData[
  FormBox[GridBox[{
     {"\[Kappa]", "=", 
      RowBox[{
       SubscriptBox["lim", 
        RowBox[{"\[Delta]", "->", "0"}]], 
       RowBox[{
        SubscriptBox["sup", 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"<", "\[Delta]"}]}]], 
        FractionBox[
         RowBox[{"||", 
          RowBox[{
           RowBox[{"f", "(", 
            RowBox[{"x", "+", "\[Delta]x"}], ")"}], "-", 
           RowBox[{"f", "(", "x", ")"}]}], "||", 
          RowBox[{"/", 
           RowBox[{"||", 
            RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"/", 
           RowBox[{"||", "x", "||"}]}]}]]}]}]},
     {" ", "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["sup", "\[Delta]x"], 
        FractionBox[
         RowBox[{"||", "\[Delta]f", "||", 
          RowBox[{"/", 
           RowBox[{"||", 
            RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
         RowBox[{"||", "\[Delta]x", "||", 
          RowBox[{"/", 
           RowBox[{"||", "x", "||"}]}]}]]}], "=", 
       FractionBox[
        RowBox[{"||", 
         RowBox[{"J", "(", "x", ")"}], 
         SubscriptBox["||", "ind"]}], 
        RowBox[{"||", 
         RowBox[{"f", "(", "x", ")"}], "||", 
         RowBox[{"/", 
          RowBox[{"||", "x", "||"}]}]}]]}]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fdaec2e3-7872-43b9-b174-ee6137da5ba6"]
}], "Subsubsection",
 CellChangeTimes->{{3.852477408901309*^9, 3.8524774954342546`*^9}, {
  3.852477529925613*^9, 3.852477640723259*^9}, {3.852477751844701*^9, 
