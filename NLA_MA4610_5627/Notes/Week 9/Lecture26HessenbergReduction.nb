Cell[TextData[{
 "The Hessenberg command in Mathematica returns Q and H.  With ",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     RowBox[{"Q", "\[Transpose]"}], ".", "A", ".", "Q"}]}], TraditionalForm]],
  ExpressionUUID->"8c7c1a42-8fd2-4db3-a000-3ea150504b94"],
 ".  It does not give positive sub diagonals! "
}], "Text",
 CellChangeTimes->{{3.856349090014181*^9, 3.85634914000412*^9}, {
  3.856349274501587*^9, 3.856349292112361*^9}, {3.856435537972082*^9, 
  3.856435549454563*^9}},ExpressionUUID->"14858f16-cd10-42bc-afbc-\
0cf26e2a9ae7"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"m", "=", "23"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"RandomComplex", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "1"}], "-", "I"}], ",", 
       RowBox[{"1", "+", "I"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "m"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Q", ",", "H"}], "}"}], "=", 
   RowBox[{"HessenbergDecomposition", "[", "A", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"QhQ", "=", 
   RowBox[{"Chop", "[", 
    RowBox[{
     RowBox[{"ConjugateTranspose", "[", "Q", "]"}], ".", "Q"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"QhAQ", "=", 
    RowBox[{"Chop", "[", 
