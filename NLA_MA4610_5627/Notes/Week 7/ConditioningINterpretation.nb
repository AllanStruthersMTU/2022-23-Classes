Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"F", "'"}], 
     RowBox[{"(", "0", ")"}], " ", "where", " ", 
     RowBox[{"F", "(", "t", ")"}]}], " ", "=", 
