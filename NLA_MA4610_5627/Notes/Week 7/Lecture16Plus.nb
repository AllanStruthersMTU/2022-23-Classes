  "a224907c-b243-444d-8032-cd585a4de8be"],
 " says ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", "(", "x", ")"}], "=", 
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{"\[Delta]", "->", "0"}]], 
     RowBox[{
      SubscriptBox["sup", 
       RowBox[{"||", 
        RowBox[{"x", "-", 
         OverscriptBox["x", "~"]}], "||", 
        RowBox[{"<", "\[Delta]"}]}]], 
      FractionBox[
       RowBox[{"||", 
        RowBox[{
         RowBox[{"f", "(", 
          OverscriptBox["x", "~"], ")"}], "-", 
         RowBox[{"f", "(", "x", ")"}]}], "||", 
        RowBox[{"/", 
         RowBox[{"||", 
          RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
       RowBox[{"||", 
        RowBox[{"x", "-", 
         OverscriptBox["x", "~"]}], "||", 
