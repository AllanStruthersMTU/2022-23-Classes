Cell[TextData[{
 "Relative Condition Number\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Kappa]", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["lim", 
       RowBox[{"\[Delta]", "->", "0"}]], 
      RowBox[{
       SubscriptBox["sup", 
        RowBox[{"||", "\[Delta]x", "||", 
         RowBox[{"<", "\[Delta]"}]}]], 
       FractionBox[
        RowBox[{"||", 
         RowBox[{
          RowBox[{"f", "(", 
           RowBox[{"x", "+", "\[Delta]x"}], ")"}], "-", 
          RowBox[{"f", "(", "x", ")"}]}], "||", 
         RowBox[{"/", 
          RowBox[{"||", 
           RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
        RowBox[{"||", "\[Delta]x", "||", 
         RowBox[{"/", 
          RowBox[{"||", "x", "||"}]}]}]]}]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["sup", "\[Delta]x"], 
       FractionBox[
        RowBox[{"||", "\[Delta]f", "||", 
         RowBox[{"/", 
          RowBox[{"||", 
           RowBox[{"f", "(", "x", ")"}], "||"}]}]}], 
        RowBox[{"||", "\[Delta]x", "||", 
         RowBox[{"/", 
          RowBox[{"||", "x", "||"}]}]}]]}], "=", 
      FractionBox[
       RowBox[{"||", 
        RowBox[{"J", "(", "x", ")"}], 
        SubscriptBox["||", "ind"]}], 
       RowBox[{"||", 
        RowBox[{"f", "(", "x", ")"}], "||", 
        RowBox[{"/", 
         RowBox[{"||", "x", "||"}]}]}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"c06be639-92a5-46da-8fe9-d0ae78189c04"]
}], "Item",
