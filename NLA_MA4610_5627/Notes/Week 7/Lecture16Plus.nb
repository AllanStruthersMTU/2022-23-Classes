      RowBox[{"\[Kappa]", "(", "x", ")"}], "||", 
      RowBox[{"f", "(", "x", ")"}], "||", 
      RowBox[{"O", "(", "\[Epsilon]", ")"}]}]}]}], TraditionalForm]],
