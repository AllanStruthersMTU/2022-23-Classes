Cell[TextData[{
 "In words:\nTake all the singular values above the threshold and discount \
them by the threshold \[OpenCurlyDoubleQuote]t\[CloseCurlyDoubleQuote], call \
the vector v.  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"||", "v", "||", 
    RowBox[{"\[GreaterEqual]", "\[Beta]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "38d00c8f-c56f-426d-b5ca-77282750f089"],
 " otherwise scale ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "84385b0e-8066-4973-b6fa-4caf7aa76e13"],
 " so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"||", 
     RowBox[{"\[Alpha]", " ", "v"}], "||"}], "=", "\[Beta]"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "95fc9f72-fec3-423d-8a9f-4aa5ec6cfaaa"]
}], "Text",
