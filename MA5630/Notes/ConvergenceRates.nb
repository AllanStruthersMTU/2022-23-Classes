     RowBox[{"{", 
      RowBox[{
       RowBox[{"z", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"b", "*", "fa"}], "-", 
           RowBox[{"a", "*", "fb"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"fa", "-", "fb"}], ")"}]}]}], ",", "fz"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"fz", "=", 
       RowBox[{"f", "[", "z", "]"}]}], ";", "  ", "\[IndentingNewLine]", 
      RowBox[{"If", "[", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"f", "[", "z", "]"}], "*", "fa"}], ">", "0"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"z", ",", "b"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"fz", ",", "fb"}], "}"}]}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"a", ",", "z"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"fa", ",", "fz"}], "}"}]}], "}"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
