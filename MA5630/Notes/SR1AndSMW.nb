 RowBox[{
  RowBox[{"Norm", "[", 
   RowBox[{
    RowBox[{"Inverse", "[", 
     RowBox[{"A", "+", 
      RowBox[{"U", ".", "CMat", ".", "V"}]}], "]"}], "-", "SMW"}], "]"}], "/", 
