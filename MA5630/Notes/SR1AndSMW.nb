Cell[TextData[{
 "I want to approximately minimize ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "53e5e79e-8be7-490f-beb6-ca6f73b88818"],
 " without ever evaluating the second derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Del]", "2"], "f"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6585b54d-9c89-4451-8447-c383dd4f6b4e"],
 ".  To avoid all sorts of complications I am going to assume that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "84a85b5b-3d4e-49d2-890c-dc5fce1a9940"],
 " is strictly convex i.e.\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{
      RowBox[{"\[Alpha]", " ", "x"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Alpha]"}], ")"}], "y"}]}], ")"}], "<", 
    RowBox[{
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"f", "(", "x", ")"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "\[Alpha]"}], ")"}], 
      RowBox[{"f", "(", "y", ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "be7685b1-cbcf-4a05-b33a-69d4dc3a64fc"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "\[Element]", 
    RowBox[{"(", 
     RowBox[{"0", ",", "1"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b2319292-1291-4134-8cab-376eb4a2c15d"],
 "\nThis ensures that there is a single unique minimizer ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "*"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f1e7ed3d-d688-4206-a570-9cf9da456c72"],
 ".  I am going to make sure that this is true in an example by adding a \
scalar multiple of the quadratic term ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ".", "x"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e2c4f325-cba3-4cdb-95e9-430ced3b6a26"],
 " to a small perturbation."
}], "Text",
 CellChangeTimes->{{3.8844316767752953`*^9, 3.884431757177333*^9}, {
  3.8844317966311507`*^9, 3.8844320300898857`*^9}, {3.8844321361542435`*^9, 
  3.8844324877819524`*^9}},ExpressionUUID->"e511f356-f1db-4476-b6b4-\
d45272abb973"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_List", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"0.5", " ", 
    RowBox[{"x", ".", "x"}]}], "+", 
   RowBox[{"0.05", "*", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"x", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "+", 
       RowBox[{"2", 
        RowBox[{"x", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}]}], ")"}], "2"]}], "+", 
   RowBox[{"0.05", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"x", "+", 
          RowBox[{"Reverse", "[", "x", "]"}]}], "]"}], "*", 
        RowBox[{"Sin", "[", 
         RowBox[{"RotateLeft", "[", 
          RowBox[{"x", ",", "1"}], "]"}], "]"}]}], "-", 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{"RotateLeft", "[", 
          RowBox[{"x", ",", "2"}], "]"}], "]"}], "*", 
        RowBox[{"Reverse", "[", "x", "]"}]}]}], ")"}], ".", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"x", "-", 
         RowBox[{"Reverse", "[", "x", "]"}]}], "]"}], "-", 
       RowBox[{"Cos", "[", 
        RowBox[{"ArcTan", "[", 
         RowBox[{
          RowBox[{"Reverse", "[", "x", "]"}], "+", "x"}], "]"}], "]"}]}], 
      ")"}]}]}]}]}]], "Input",
 CellChangeTimes->{{3.8844324926683397`*^9, 3.88443266262226*^9}, {
