Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MSAlg3Point2", "[", 
    RowBox[{"B_", ",", "g_", ",", "\[CapitalDelta]_"}], "]"}], "[", 
   "\[Lambda]_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "B", "]"}]}], ",", "R", ",", "p", ",", "q"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"Assumption", " ", "is", " ", "that", " ", "\[Lambda]"}], ">", 
      RowBox[{"0", " ", "and", 
       FormBox[
        RowBox[{"B", "+", 
         RowBox[{"\[Lambda]", " ", "I"}]}],
        TraditionalForm], " ", "is", " ", "SPD", " ", "and", " ", 
       "\[CapitalDelta]"}], ">", "0"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Not", " ", "worried", " ", "about", " ", "efficiency", " ", "right", 
      " ", "now"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Not", " ", "worried", " ", "about", " ", "the", " ", "safegaurding"}], 
     " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"R", "=", 
      RowBox[{"CholeskyDecomposition", "[", 
       RowBox[{"B", "+", 
        RowBox[{"\[Lambda]", " ", 
         RowBox[{"IdentityMatrix", "[", "n", "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{
        RowBox[{"B", "+", 
         RowBox[{"\[Lambda]", " ", 
          RowBox[{"IdentityMatrix", "[", "n", "]"}]}]}], ",", 
        RowBox[{"-", "g"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"q", "=", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{
        RowBox[{"R", "\[Transpose]"}], ",", "p"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Lambda]", "+", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{"Norm", "[", "p", "]"}], 
          RowBox[{"Norm", "[", "q", "]"}]], ")"}], "2"], 
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          RowBox[{"Norm", "[", "p", "]"}], "-", "\[CapitalDelta]"}], 
         "\[CapitalDelta]"], ")"}]}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
