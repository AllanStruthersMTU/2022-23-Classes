     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"M0", ",", "M1"}], "}"}], "=", 
      RowBox[{"TRSMatricesBig", "[", 
       RowBox[{"A", ",", "B", ",", "g", ",", "\[CapitalDelta]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Does", " ", "not", " ", "treat", " ", "or", " ", "test", " ", "the", 
