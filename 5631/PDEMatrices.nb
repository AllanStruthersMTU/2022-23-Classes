Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"uSol", "[", "x_", "]"}], ":=", 
   RowBox[{
    RowBox[{"x", " ", 
     RowBox[{"Cos", "[", "x", "]"}]}], "-", 
    SuperscriptBox["x", "3"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"uSol", "[", "0", "]"}], ",", 
     RowBox[{"uSol", "[", "1", "]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], "=", 
   RowBox[{"-", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"uSol", "[", "x", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"m", "=", "12"}], ";", " ", 
  RowBox[{"\[CapitalDelta]x", "=", 
   RowBox[{"1.0", "/", 
    RowBox[{"(", 
     RowBox[{"m", "+", "1"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"SparseArray", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"Band", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1"}], "}"}], "]"}], "->", 
        RowBox[{"-", "2"}]}], ",", 
       RowBox[{
        RowBox[{"Band", "[", 
         RowBox[{"{", 
          RowBox[{"2", ",", "1"}], "}"}], "]"}], "->", "1"}], ",", 
       RowBox[{
        RowBox[{"Band", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}], "]"}], "->", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "m"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", 
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox["\[CapitalDelta]x", "2"]}], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"i", " ", "\[CapitalDelta]x"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "]"}]}]}], ";", 
  RowBox[{
   RowBox[{"b", "\[LeftDoubleBracket]", 
    RowBox[{"{", 
     RowBox[{"1", ",", "m"}], "}"}], "\[RightDoubleBracket]"}], "-=", 
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u", "=", 
   RowBox[{"LinearSolve", "[", 
    RowBox[{"A", ",", "b"}], "]"}]}], ";", " ", 
  RowBox[{"uBC", "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{"{", "\[Alpha]", "}"}], ",", "u", ",", 
     RowBox[{"{", "\[Beta]", "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"TabView", "[", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\"\<Sol\>\"", "->", 
     RowBox[{"Show", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Plot", "[", " ", 
        RowBox[{
         RowBox[{"uSol", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "0", ",", "1"}], "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", "Red"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ListPlot", "[", 
        RowBox[{"uBC", ",", 
         RowBox[{"DataRange", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PlotLabel", "\[Rule]", "m"}]}], "\[IndentingNewLine]", 
      "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Error\>\"", "->", 
     RowBox[{"ListPlot", "[", 
      RowBox[{
       RowBox[{"u", "-", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"uSol", "[", 
           RowBox[{"i", " ", "\[CapitalDelta]x"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "m"}], "}"}]}], "]"}]}], ",", 
       RowBox[{"PlotLabel", "->", "m"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "}"}], "]"}], "\[IndentingNewLine]"}], "Input",
