Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "=", 
    RowBox[{"2", ":", "k"}]}], TraditionalForm]],ExpressionUUID->
  "7c185f1c-7b81-4215-9a07-d08257563d3b"],
 "  the columns satisfy\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", ".", 
     SubscriptBox["q", "j"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Beta]", "j"], 
      SubscriptBox["q", 
       RowBox[{"j", "-", "1"}]]}], "+", 
