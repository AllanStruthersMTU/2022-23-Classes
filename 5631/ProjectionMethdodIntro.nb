         SubscriptBox["x", "0"], "+", "\[ScriptCapitalK]"}]}]], " ", 
      RowBox[{"A", ".", 
       RowBox[{"(", 
        RowBox[{"x", "-", 
         SubscriptBox["x", "*"]}], ")"}], ".", 
       RowBox[{"(", 
        RowBox[{"x", "-", 
         SubscriptBox["x", "*"]}], ")"}]}]}], "=", 
     RowBox[{
      SubscriptBox["argmin", 
       RowBox[{"x", "\[Element]", 
        RowBox[{
         SubscriptBox["x", "0"], "+", "\[ScriptCapitalK]"}]}]], " ", 
      SqrtBox[
       RowBox[{"A", ".", 
        RowBox[{"(", 
         RowBox[{"x", "-", 
