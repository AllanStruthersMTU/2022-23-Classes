Cell[TextData[{
 "P5 .6 If ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", ".", "\[ScriptCapitalK]"}], "=", 
    RowBox[{"\[ScriptCapitalK]", "=", "\[ScriptCapitalL]"}]}], 
